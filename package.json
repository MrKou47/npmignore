{
  "name": "npmignore",
  "displayName": "npmignore",
  "description": "Add file to .npmignore",
  "version": "0.0.1",
  "publisher": "mrkou47",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [{
      "command": "extension.npmIgnore",
      "title": "Add to .npmignore"
    }],
    "menus": {
      "explorer/context": [{
        "command": "extension.npmIgnore",
        "group": "navigation"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}